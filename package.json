{
  "name": "gatsby-remark-a11y-emoji",
  "description": "Gatsby Plugin to make Emoji in Markdown accessible. Wraps Emoji in a <span>-Tag with role and aria-label attributes.",
  "version": "1.0.1",
  "license": "MIT",
  "author": "Florian Eckerstorfer <florian@eckerstorfer.net>",
  "bugs": {
    "url": "https://github.com/florianeckerstorfer/gatsby-remark-a11y-emoji/issues"
  },
  "scripts": {
    "build": "babel --out-dir . --ignore __tests__ src",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "test": "jest --coverage"
  },
  "homepage": "https://github.com/florianeckerstorfer/gatsby-remark-a11y-emoji",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "remark",
    "emoji",
    "a11y"
  ],
  "main": "index.js",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "remark": "^11.0.1",
    "unist-util-visit": "^2.0.0"
  },
  "dependencies": {
    "emoji-regex": "^8.0.0",
    "gemoji": "^4.2.1"
  }
}
